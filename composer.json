{
    "name": "gfra54/snitchlock",
    "description": "Report when two people might be editing the same element (eg entry, category, or global) or field",
    "type": "craft-plugin",
    "version": "3.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "snitchlock"
    ],
    "support": {
        "docs": "https://github.com/gfra54/craft-snitchlock",
        "issues": "https://github.com/gfra54/craft-snitchlock/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gilles FRANCOIS",
            "homepage": "https://sopress.net"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
            "gfra54\\snitchlock\\": "src/"
        }
    },
    "extra": {
        "name": "SnitchLock",
        "handle": "snitchlock",
        "hasCpSettings": false,
        "hasCpSection": false,
        "documentationUrl": "https://github.com/gfra54/craft-snitchlock",
        "changelogUrl": "https://github.com/gfra54/craft-snitchlock",
        "components": {
            "collision": "gfra54\\snitchlock\\services\\Collision"
        },
        "class": "gfra54\\snitchlock\\SnitchLock"
    }
}
